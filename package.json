{
  "name": "shell",
  "version": "1.8.4",
  "engines": {
    "node": "^10.24.1",
    "npm": "^6.4.1"
  },
  "description": "A react & koa based ticket store.",
  "main": "backend/server.js",
  "scripts": {
    "analyze": "webpack --config webpack.dev.config.js --profile --progress --json > stats.json",
    "buildlocal": "npm run clear-static && NODE_ENV=development webpack --config webpack.local.config.js --watch",
    "buildprod": "npm run clear-static && NODE_ENV=production webpack --config webpack.prod.config.js",
    "clear-static": "rm -rf ./backend/static",
    "db:migrate:undo:all": "sequelize db:migrate:undo:all",
    "db:migrate:undo": "sequelize db:migrate:undo",
    "db:migrate": "sequelize db:migrate",
    "db:seed:all": "sequelize db:seed:all",
    "db:seed:undo:all": "sequelize db:seed:undo:all",
    "debug": "node --inspect=0.0.0.0 backend/server.js",
    "e2e-testing": "cypress run",
    "lint-diff": "eslint-git-changes --json --jsx --js",
    "lint": "eslint . --ext .js --ext .jsx",
    "show-analysis": "webpack-bundle-analyzer stats.json backend/static",
    "start": "node backend/server.js",
    "test:coverage": "NODE_ENV=test jest --coverage --forceExit",
    "test:watch": "NODE_ENV=test jest --watch",
    "test": "NODE_ENV=test jest --runInBand",
    "update-maxmind": "npm_package_config_update=true node ./node_modules/geoip-lite/scripts/updatedb.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ticketevolution/shell.git"
  },
  "author": "Tal Goldfus <tal@ticketevolution.com> (Store V1 Store -Paul Pucciarelli <paul@ticketevolution.com>)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ticketevolution/shell/issues"
  },
  "homepage": "https://github.com/ticketevolution/shell#readme",
  "dependencies": {
